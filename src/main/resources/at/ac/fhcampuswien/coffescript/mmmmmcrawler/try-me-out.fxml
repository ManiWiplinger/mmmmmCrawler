<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" spacing="20.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="at.ac.fhcampuswien.coffescript.mmmmmcrawler.TryMeOutController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
    <ImageView fitHeight="50.0" fitWidth="380.0" layoutX="110.0" layoutY="25.0">
        <image>
            <Image url="@../../../../../graphics/crawler-image.png" />
        </image>
    </ImageView>
    <Label fx:id="baseCrawl" />
    <Button onAction="#onHelloButtonClick" text="Crawl" />
    <Button onAction="#tiefe1" text="Crawl tiefe 1" />
    <Button onAction="#tiefe2" text="Crawl tiefe 2" />
    <Button onAction="#back" text="Back" />
    <TextArea fx:id="ta" editable="false" minHeight="300" />
</VBox>
