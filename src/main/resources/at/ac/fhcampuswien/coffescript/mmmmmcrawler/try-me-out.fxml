<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>

<VBox alignment="CENTER" spacing="20.0" xmlns="http://javafx.com/javafx/11.0.2" xmlns:fx="http://javafx.com/fxml/1" fx:controller="at.ac.fhcampuswien.coffescript.mmmmmcrawler.TryMeOutController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
    <ImageView fitHeight="65.0" fitWidth="240.0" layoutX="14.0" layoutY="39.0">
        <image>
            <Image url="@../../../../../graphics/crawler-image.png" />
        </image>
    </ImageView>
    <Label fx:id="baseCrawl" />
    <Button onAction="#onHelloButtonClick" prefHeight="26.0" prefWidth="86.0" text="Crawl" />
    <Button onAction="#tiefe1" prefHeight="26.0" prefWidth="86.0" text="Crawl tiefe 1" />
    <Button onAction="#tiefe2" prefHeight="26.0" prefWidth="86.0" text="Crawl tiefe 2" />
    <Button onAction="#back" prefHeight="26.0" prefWidth="86.0" text="Back" />
    <TextArea fx:id="ta" editable="false" minHeight="300" />
</VBox>
