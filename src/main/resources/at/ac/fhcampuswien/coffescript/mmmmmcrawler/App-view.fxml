<VBox alignment="CENTER" spacing="20.0" xmlns:fx="http://javafx.com/fxml"
      fx:controller="at.ac.fhcampuswien.coffescript.mmmmmcrawler.AppController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
    </padding>
    <Label fx:id="baseCrawl"/>
    <Button text="Crawl" onAction="#onHelloButtonClick"/>
    <Button text="Crawl tiefe 1" onAction="#tiefe1"/>
    <Button text="Crawl tiefe 2" onAction="#tiefe2"/>
    <TextArea fx:id="ta" minHeight="300" editable="false"/>
</VBox>
